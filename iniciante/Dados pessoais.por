programa {
  funcao inicio() {

    //Declaração de Variáveis
    cadeia nome
    inteiro idade, cpf, telefone

    //Entrada
    escreva("Nome: ")
    leia(nome)
    escreva("Idade: ")
    leia(idade)
    escreva("CPF: ")
    leia(cpf)
    escreva("Telefone: ")
    leia(telefone)

    //Saída
    escreva("Olá ", nome, ", você tem ", idade, " anos. Seu CPF é ", cpf,", e seu número é ", telefone)
  }
}
