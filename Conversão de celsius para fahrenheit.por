programa {
  funcao inicio() {

    //Definição das variáveis
    inteiro temperatura, calculo_fahrenheit

    //Entrada
    escreva("Qual a temperartura em °C: ")
    leia(temperatura)

    //Processamento
    calculo_fahrenheit = (temperatura * 1.8) + 32

    //Saída
    escreva("A temperatura em Fahrenheit é: ", calculo_fahrenheit, " °F")
    
  }
}
