programa {
  funcao inicio() {

    //Definição das variáveis
    real altura, base, area

    //Entrada
    escreva("Qual a dimensão da base do terreno: ")
    leia(base)
    escreva("Qual a dimensão da altura do terreno: ")
    leia(altura)

    ///Processamento
    area = altura * base 

    //Saída
    escreva("A área total do terreno é: ", area, " m²")

  }
}
